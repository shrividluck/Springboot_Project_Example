package com.shri.mtts.entity;

import java.util.List;

public interface Movie {
	long getMovieId();	
	String getMovieName();
	String getMovieGenre();  // new name..no other change
	String getMovieCastCrew();
	String getMovieRatings();
	String getMovieReviews();	
	String getSynopsis();
	List<String> getTheatersPlayingMovie();  // new...get list of theaters playing the movie.
}
